@model IEnumerable<MyPostTest.Models.Profiling>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Education)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Account)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Education.Degree)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Account.NIK)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.NIK">Edit</a> |
                <a asp-action="Delete" asp-route-id="@item.NIK">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
